{
  "name": "rsschool_react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": " cross-env NODE_ENV=production ts-node server.ts",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "test": "vitest",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome && nyc report --reporter=text-summary",
    "test:cy": "start-server-and-test dev http://localhost:5173 cy:run",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.9.3",
    "@testing-library/user-event": "~14.4.3",
    "@types/node": "~18.15.11",
    "@types/react-transition-group": "~4.4.5",
    "classnames": "~2.3.2",
    "compression": "~1.7.4",
    "express": "~4.18.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-hook-form": "~7.43.9",
    "react-redux": "~8.0.5",
    "react-router-dom": "~6.10.0",
    "react-transition-group": "~4.4.5",
    "sirv": "~2.0.2",
    "vite-plugin-svgr": "~2.4.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "~3.10.4",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~14.0.0",
    "@types/compression": "~1.7.2",
    "@types/cypress__code-coverage": "~3.10.0",
    "@types/express": "~4.17.16",
    "@types/react": "~18.0.31",
    "@types/react-dom": "~18.0.11",
    "@typescript-eslint/eslint-plugin": "~5.57.0",
    "@typescript-eslint/parser": "~5.57.0",
    "@vitejs/plugin-react": "~3.1.0",
    "@vitest/coverage-c8": "~0.29.8",
    "autoprefixer": "~10.4.14",
    "cross-env": "~7.0.3",
    "cross-fetch": "~3.1.5",
    "cypress": "~12.10.0",
    "cypress-vite": "~1.3.2",
    "eslint": "~8.37.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-cypress": "~2.13.2",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-testing-library": "~5.10.2",
    "husky": "~8.0.3",
    "jsdom": "~21.1.1",
    "msw": "~1.2.1",
    "postcss": "~8.4.21",
    "prettier": "~2.8.7",
    "prettier-plugin-tailwindcss": "~0.2.6",
    "start-server-and-test": "~2.0.0",
    "tailwindcss": "~3.3.1",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5",
    "vite": "~4.2.1",
    "vite-plugin-eslint": "~1.8.1",
    "vite-plugin-istanbul": "~4.0.1",
    "vitest": "~0.29.8"
  }
}
